pipeline{
    agent any
    stages{
    tools {
        maven 'm1'
        }
        
        
        stage(build){
            steps{
               sh 'mvn clean install -DskipTests'
               echo "skipping test"
               echo "hi13335441156546miq"
            }
        }
        stage(test){
            steps{
               sh 'mvn test'
            }
        post{
            always{
                junit "target/surefire-reports/*.xml"
                archiveArtifacts 'target/*.jar'
            }    
            }
        }
        stage(deploy){
            steps{
                sh './scripts/deliver.sh'
            }
        }
    }
}
